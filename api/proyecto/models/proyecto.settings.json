{
  "kind": "collectionType",
  "collectionName": "proyectos",
  "info": {
    "name": "proyecto",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titulo": {
      "type": "string"
    },
    "descripcion": {
      "type": "richtext"
    },
    "ciudad": {
      "type": "string"
    },
    "meta": {
      "type": "component",
      "repeatable": true,
      "component": "informacion.meta"
    },
    "autor": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "proyectos"
    },
    "Ano": {
      "type": "integer"
    },
    "miniatura": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "imagenes": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "slug": {
      "type": "string",
      "unique": true
    },
    "categorias": {
      "via": "proyectos",
      "model": "categoria"
    }
  }
}
